{
    "name": "kiurd/role-based-access-control",
    "description": "Dynamic role-permission system for Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kiurd\\RolePermissions\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kiurd\\RolePermissions\\RolePermissionsServiceProvider"
            ],
            "aliases": {
                "RolePermissions": "Kiurd\\RolePermissions\\Facades\\RolePermissionsFacade"
            }
        }
    },
    "require": {
        "php": "^8.0|^8.1|^8.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
